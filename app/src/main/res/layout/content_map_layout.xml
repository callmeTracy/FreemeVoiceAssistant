<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minHeight="@dimen/music_list_item_min_height">

    <TextView
        android:id="@+id/map"
        style="@style/buttonText"
        android:layout_width="@dimen/menuguidebutton_margin_width"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="@dimen/calling_cancel_margin_top"
        android:background="@drawable/single_cancel_bg"
        android:clickable="true"
        android:text="@string/please_reviewmap" />
</RelativeLayout>